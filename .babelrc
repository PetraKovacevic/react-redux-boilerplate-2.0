{
    "presets": [
        "babel-preset-env",
        "es2015",
        "react",
        "stage-1"
    ],
    "env": {
        "production": {
            "presets": [
                "react-optimize"
            ]
        }
    },
    "plugins": [
        "transform-es3-property-literals",
        [
            "babel-plugin-root-import",
            [
                {
                    "rootPathPrefix": "@",
                    "rootPathSuffix": "src"
                }
            ]
        ],
        // [
        //     "import",
        //     {
        //         "libraryName": "react-bootstrap",
        //         "style": false
        //     }
        // ]
    ]
}
